{
    "name": "parth-vadgama/lx-blade-components",
    "description": "Laravel x-blade components is a component library built to support livewire and tailwind based project.",
    "keywords": [
        "laravel",
        "components",
        "laravel-components",
        "blade-components",
        "x-blade-components"
    ],
    "homepage": "https://github.com/parth-vadgama/lx-blade-components",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ParthVadgama\\LxBladeComponents\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Parth Gajjar",
            "email": "gajjar.parth1811@gmail.com",
            "homepage": "https://github.com/parth-vadgama",
            "role": "Developer"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "ParthVadgama\\LxBladeComponents\\Providers\\LxBladeComponentsServiceProvider"
            ]
        }
    },
    "require": {
        "illuminate/support": "^9.35",
        "illuminate/view": "^9.35",
        "illuminate/console": "^9.35"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
